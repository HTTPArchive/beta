	"chrome-ux-report": {
		"name": "Chrome User Experience Report: Loading",
		"summary": "Loading performance as experienced by real-world Chrome users across a diverse set of real-world hardware and network conditions. The provided data is powered by real user measurement (RUM) of key loading user experience metrics across the public web, provided by the <a href=\"https://developers.google.com/web/tools/chrome-user-experience-report/\">Chrome User Experience Report</a>.",
		"metrics": [
			"cruxFp",
			"cruxFcp",
			"cruxDcl",
			"cruxOl"
		],
		"minDate": "2017_10_01",
		"maxDate": "2017_12_01",
		"datePattern": ".*_01$",
		"timeseries": {
			"enabled": false
		},
		"graphic": {
			"bgcolor": "#97b5b9",
			"primary": {
